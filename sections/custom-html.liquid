<section class="Section Section--spacingNormal" id="section-{{ section.id }}" style="background-color:{{ section.settings.bgcolor }}; color:{{ section.settings.txtcolor }}">
  <div class="Container">
    {%- if section.settings.subheading != blank or section.settings.title != blank -%}
      <header class="SectionHeader SectionHeader--center">
        {%- if section.settings.subheading != blank -%}
          <h3 class="SectionHeader__SubHeading Heading u-h6">{{ section.settings.subheading | escape }}</h3>
        {%- endif -%}

        {%- if section.settings.title != blank -%}
          <h2 class="SectionHeader__Heading Heading u-h1">{{ section.settings.title | escape }}</h2>
        {%- endif -%}
      </header>
    {%- endif -%}
    {% if page.id == 99733373082 %}
      <div class="Rte" style="text-align: center;margin:0 auto;max-width:1349px;font-size: 32px;max-width:900px;font-style: normal;font-weight: 600;line-height: normal;">
        {{ section.settings.html }}
      </div>
    {% else %}
      <div class="Rte" style="text-align: center;">
        {{ section.settings.html }}
      </div>
    {% endif %}
  </div>
  {% if page.id == 99733373082 %}
    <div class="bottom_container">
      <p>nothing</p>
    </div>
  {% endif %}
</section>
<style>
.bottom_container
{
    height:300px;
    background: {{ section.settings.bgcolor }} url('https://cdn.shopify.com/s/files/1/0459/6563/9834/files/Vector.png?v=1702283714') bottom right no-repeat;
}
</style>
{% schema %}
{
  "name": "Custom HTML",
  "class": "shopify-section--bordered",
  "settings": [
    {
      "type": "text",
      "id": "subheading",
      "label": "Sub-heading",
      "default": "Sub-heading"
    },
    {
      "type": "text",
      "id": "title",
      "label": "Heading",
      "default": "Your HTML"
    },
    {
      "type": "html",
      "id": "html",
      "label": "HTML content",
      "default": "<p>Write your own custom HTML content.</p>"
    },
    {
      "type": "color",
      "id": "bgcolor",
      "label": "Background color",
      "default": "#FFFFFF"
    },
    {
      "type": "color",
      "id": "txtcolor",
      "label": "Text color",
      "default": "#FFFFFF"
    }
  ],
  "presets": [
    {
      "category": "Advanced",
      "name": "Custom HTML",
      "settings": {}
    }
  ]
}
{% endschema %}
